{
	// See https://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "2.0.0",
	"tasks": [
		{
			"label": "CMake: Generate ZIP Package",
			"type": "shell",
			"command": "cpack",
			"args": [
				"-G",
				"ZIP"
			],
			"group": "build",
			"options": {
				"cwd": "${workspaceFolder}/build"
			},
			"problemMatcher": []
		}
	]
}
